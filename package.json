  "private": true,
  "scripts": {
    "dev": "pnpm db:check && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "db:generate": "drizzle-kit generate",
